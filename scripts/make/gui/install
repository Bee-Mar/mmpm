#!/bin/bash
source ./scripts/make/constants

printf "Installing GUI Static Files\n"

log_notice

log_action "Installing MMPM GUI Static Files"

printf " -- extracting and copying files to installation destination"
log_and_eval "sudo rm -rfv /var/www/mmpm"
log_and_eval "sudo tar xvf mmpm-gui-$MMPM_LATEST_VERSION.tar.gz -C /"
log_and_eval "rm -vf mmpm-gui-$MMPM_LATEST_VERSION.tar.gz"
_done_

log_action "Installed MMPM GUI Static Files"
printf "Installed MMPM GUI Static Files\n"
